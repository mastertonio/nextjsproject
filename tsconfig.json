{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@redux/*": ["app/redux/*"],
      "@dashboard/*": ["app/dashboard/*"],
      "@core/*": ["app/core/*"],
      "@styles/*": ["styles/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "app/core/components/data-grid/DataGrid.js", "app/core/components/data-grid/components/ReactTable/index.js", "app/core/components/data-grid/components/ReactTable/filterTypes.js"],
  "exclude": ["node_modules"]
}
